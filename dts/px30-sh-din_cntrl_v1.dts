/dts-v1/;
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/pinctrl/rockchip.h>
#include <dt-bindings/input/input.h>
#include "px30.dtsi"

/ {
	model = "Smart Home DIN Controller v1";
	compatible = "inmys,px30-sh-din_cntrl_v1", "rockchip,px30";

	chosen {
		stdout-path = "serial3:115200n8";
	};
	aliases {
		ethernet0 = &gmac;
		rtc0 = &spirtc;
		rtc1 = &rk817;
	};

	
	reserved-memory {
                #address-cells = <2>;
                #size-cells = <2>;
                ranges;

                ramoops: ramoops@8000000 {
                        compatible = "ramoops";
                        reg = <0x0 0x8000000 0x0 0xa0000>;
                        record-size = <0x20000>;
                        console-size = <0x80000>;
                        ftrace-size = <0x00000>;
                        pmsg-size = <0x00000>;
                };
        };

	/* SH_PX30_DIN_CNTRL pwr */
	vbus: vbusv {
		compatible = "regulator-fixed";
		regulator-name = "vbus";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
	};

	
	/* som pwr */
	vccsys: vccsys {
		compatible = "regulator-fixed";
		regulator-name = "vcc3v8_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <3800000>;
		regulator-max-microvolt = <3800000>;
		vin-supply = <&vbus>;
	};
	vcc2v2_sys: vcc2v2_sys {
		/* PAM2305CGFADJ */
		compatible = "regulator-fixed";
		regulator-name = "vcc2v2_sys";
		regulator-min-microvolt = <2200000>;
		regulator-max-microvolt = <2200000>;
		vin-supply = <&vccsys>;
	};
	

	charge-animation {
		compatible = "rockchip,uboot-charge";
		rockchip,uboot-charge-on = <0>;
		rockchip,android-charge-on = <1>;
		rockchip,uboot-low-power-voltage = <3500>;
		rockchip,screen-on-voltage = <3600>;
		status = "okay";
	};

	snd {
		compatible = "simple-audio-card";
		simple-audio-card,format = "i2s";
		simple-audio-card,name = "sndcardname";
		simple-audio-card,mclk-fs = <256>;

		#address-cells = <1>;
		#size-cells = <0>;
		simple-audio-card,dai-link@0 {
			reg = <0>;
			cpu {
				sound-dai = <&i2s1_2ch>;
			};
			codec {
				sound-dai = <&rk817_codec>;
			};
		};
	};

	sdio_pwrseq: sdio-pwrseq {
		compatible = "mmc-pwrseq-simple";
		clocks = <&rk817 1>;
		clock-names = "ext_clock";
		pinctrl-names = "default";
		pinctrl-0 = <&wifi_enable_h>;

		/*
		 * On the module itself this is one of these (depending
		 * on the actual card populated):
		 * - SDIO_RESET_L_WL_REG_ON
		 * - PDN (power down when low)
		 */
		reset-gpios = <&gpio0 RK_PA2 GPIO_ACTIVE_LOW>;
		post-power-on-delay-ms = <10>;
	};

	wireless-wlan {
		status = "okay";
		compatible = "wlan-platdata";
		/*wifi_chip_type = "ap6212a";*/
		/*wifi_chip_type = "ap6212";*/
		wifi_chip_type = "ap6236";
		pinctrl-names = "default";
		pinctrl-0 = <&wifi_host_wake>;
		WIFI,host_wake_irq = <&gpio0 RK_PB2 GPIO_ACTIVE_HIGH>;
		/*bcmdhd-gpios = <&gpio0 RK_PB2 GPIO_ACTIVE_HIGH>;*/
	};

#if 0
	wireless-bluetooth {
		compatible = "bluetooth-platdata";
		clocks = <&rk817 1>;
		clock-names = "ext_clock";
		uart_rts_gpios = <&gpio1 RK_PC3 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default","rts_gpio";
		pinctrl-0 = <&uart1_rts>;
		pinctrl-1 = <&uart1_rts_gpio>;
		BT,reset_gpio = <&gpio2 RK_PB0 GPIO_ACTIVE_HIGH>;
		BT,wake_gpio = <&gpio0 RK_PA1 GPIO_ACTIVE_HIGH>;
		BT,wake_host_irq = <&gpio0 RK_PB3 GPIO_ACTIVE_HIGH>;
		status = "okay";
	};
#endif
	fiq-debugger {
		// ToDo: conflict with uart3
		status = "disabled";
	};
	
	leds{
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&led_gpio>;
		cpu_led {
			label = "cpu_led";
			gpios = <&gpio1 RK_PB0 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "heartbeat";
		};
		ch342f_reset {
			gpios = <&gpio3 RK_PC2 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
		RELAY_1 {
			gpios = <&gpio3 RK_PC3 GPIO_ACTIVE_LOW>;
			default-state = "keep";
		};
		RELAY_2 {
			gpios = <&gpio3 RK_PC4 GPIO_ACTIVE_LOW>;
			default-state = "keep";
		};
		RELAY_3 {
			gpios = <&gpio3 RK_PC5 GPIO_ACTIVE_LOW>;
			default-state = "keep";
		};
	};
};

