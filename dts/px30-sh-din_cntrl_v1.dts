			pinctrl-0 = <&i2s1_2ch_mclk>;
			hp-volume = <20>;
			spk-volume = <3>; /* 3 db oslablenie - menshe nelzya */
			capture-volume = <20>;
			status = "okay";
			mic-in-differential;
