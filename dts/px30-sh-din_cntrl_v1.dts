	bus-supply = <&vdd_logic>;
	status = "okay";
};
#endif

&cpu0 {
	cpu-supply = <&vdd_arm>;
};

&cpu1 {
        cpu-supply = <&vdd_arm>;
};

&cpu2 {
        cpu-supply = <&vdd_arm>;
};

&cpu3 {
        cpu-supply = <&vdd_arm>;
};

